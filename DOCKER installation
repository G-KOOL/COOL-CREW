22-  echo  ${USER}
23- docker ps -a
24- sudo curl -L "https://github.com/docker/compose/releases/download/1.23.2/docker-compose-$(uname -s)-$(uname -m)" -o /usr/local/bin/docker-compose
25-  curl -L https://github.com/docker/compose/releases/download/1.24.0-rc1/docker-compose-`uname -s`-`uname -m` -o /usr/local/bin/docker-compose
26-  sudo chmod +x /usr/local/bin/docker-compose
27- sudo ln -s /usr/local/bin/docker-compose /usr/bin/docker-compose

